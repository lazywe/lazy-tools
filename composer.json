{
    "name": "lazywe/lazy-tools",
    "description": "基于laravel的工具脚手架，支持数据Rsa Aes双重加密",
    "authors": [
        {
            "name": "lazywe",
            "email": "lazywe@163.com"
        }
    ],
    "require": {
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/laravel": "^5.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lazy\\Tools\\": "src"
        },
        "files": [
            "src/Tools.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lazy\\Tools\\ServiceProvider"
            ]
        }
    }
}